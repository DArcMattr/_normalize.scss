{
  "name": "_normalize.scss",
  "version": "1.3.0",
  "description": "Sass version of Normalize.css, customized with some more defaults",
  "files": [
    "_normalize.scss",
    "README.md",
    "LICENSE.md",
    "package.json",
    "assets/",
    "test.html"
  ],
  "devDependencies": {
    "clean-css": "3.4.9",
    "css-b64-images": "0.2.5",
    "node-sass": "3.4.2",
    "nodemon": "1.8.1"
  },
  "scripts": {
    "build": "css-b64-images ./_normalize.scss > _normalize-inlined.scss.tmp && node-sass --output-style=compressed --source-map=true _normalize.inlined.scss normalize.css && rm _normalize-inlined.scss.tmp",
    "install": "npm run build-sass",
    "watch": "nodemon -e scss -w ./*.scss -x 'npm run build'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DArcMattr/_normalize.scss.git"
  },
  "keywords": [
    "Sass",
    "normalize"
  ],
  "author": "Necolas",
  "main": "_normalize.scss",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DArcMattr/_normalize.scss/issues"
  },
  "homepage": "https://github.com/DArcMattr/_normalize.scss",
  "dependencies": {}
}
